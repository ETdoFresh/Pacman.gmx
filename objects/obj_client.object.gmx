        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update/Add/Remove Player Instances

if (client == eventid &amp;&amp; command == CMD_UPDATE)
{
    ds_list_clear(sync_list);
    
    var entity;
    var i = 0;
    for (var i = 0; i &lt; entities; i++)
    {
        var server_type = update_data[i, 0];
        var server_id = update_data[i, 1];

        // Search for Entity
        var found_entity = false;
        for (var j = 0; j &lt; ds_list_size(entity_list); j++)
        {
            entity = ds_list_find_value(entity_list, j);
            if (entity.server_id == server_id)
            {
                found_entity = true;
                break;
            }
        }        
        // If Entity not found, Add Entity
        if (!found_entity)
        {
            var obj_type = -1;
            if (server_type == obj_server_player) obj_type = obj_client_player;
            else if (server_type == obj_bullet) obj_type = obj_client_bullet;
            else continue;

            entity = instance_create(0, 0, obj_type);
            entity.server_id = server_id;
            entity.number = update_data[i, 6];
            ds_list_add(entity_list, entity);
            
            // Show after entity interpolation started
            if (server_type == obj_bullet &amp;&amp; is_entity_interpolation)
                entity.visible = false;
        }
        
        // Update entity
        entity.x = update_data[i, 2];
        entity.y = update_data[i, 3];
        entity.image_angle = update_data[i, 4];
        entity.image_blend = update_data[i, 5];
        ds_list_add(sync_list, entity.server_id);
    }
    
    // Remove Entities
    for (var i = 0; i &lt; ds_list_size(entity_list); i++)
    {
        var entity = ds_list_find_value(entity_list, i);
        
        // Search for Entity
        var found_entity = false;
        for (var j = 0; j &lt; ds_list_size(sync_list); j++)
        {
            if (entity.server_id == ds_list_find_value(sync_list, j))
            {
                found_entity = true;
                break;
            }
        }
        // If Entity was not updated, destroy it
        if (!found_entity)
        {
            ds_list_delete(entity_list, i);
            with (entity) instance_destroy();
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>

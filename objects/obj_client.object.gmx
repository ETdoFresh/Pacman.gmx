          <argument>
            <kind>1</kind>
            <string>/// Update/Add/Remove Player Instances

if (client == eventid &amp;&amp; command == CMD_UPDATE)
{
    ds_list_clear(sync_list);
    for (var i = 0; i &lt; entities; i++)
    {
        var incoming_list = ds_list_find_value(update_data, i);
        var server_type = ds_list_find_value(incoming_list, 0);
        var server_id = ds_list_find_value(incoming_list, 1);

        // Search for Entity
        var entity = -1;
        for (var j = 0; j &lt; ds_list_size(entity_list); j++)
        {
            entity = ds_list_find_value(entity_list, j);
            if (entity.server_id == server_id)
                break;
            entity = -1;
        }        
        // If Entity not found, Add Entity
        if (entity &lt; 0)
        {
            var obj_type = -1;
            if (server_type == obj_server_player) obj_type = obj_client_player;
            else if (server_type == obj_bullet) obj_type = obj_bullet;
            else continue;

            var _x = ds_list_find_value(incoming_list, 2);
            var _y = ds_list_find_value(incoming_list, 3);
            entity = instance_create(_x, _y, obj_type);
            entity.server_id = server_id;
            entity.number = ds_list_find_value(incoming_list, 6);
            ds_list_add(entity_list, entity);
            
            if (server_type == obj_bullet)
            {
                entity.alarm[0] = -1; // Don't destroy client bullets automatically
                entity.target_x = ds_list_find_value(incoming_list, 7);
                entity.target_y = ds_list_find_value(incoming_list, 8);
            }
        }
        
        // Update entity
        entity.x = ds_list_find_value(incoming_list, 2);
        entity.y = ds_list_find_value(incoming_list, 3);
        entity.image_angle = ds_list_find_value(incoming_list, 4);
        entity.image_blend = ds_list_find_value(incoming_list, 5);
        ds_list_add(sync_list, entity.server_id);
    }
    
    // Remove Entities
    for (var i = 0; i &lt; ds_list_size(entity_list); i++)
    {
        var entity = ds_list_find_value(entity_list, i);
        
        // Search for Entity
        var found_entity = false;
        for (var j = 0; j &lt; ds_list_size(sync_list); j++)
        {
            if (entity.server_id == ds_list_find_value(sync_list, j))
            {
                found_entity = true;
                break;
            }
        }
        // If Entity was not updated, destroy it
        if (!found_entity)
        {
            ds_list_delete(entity_list, i);
            with (entity) instance_destroy();
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>

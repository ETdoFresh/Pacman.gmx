          <argument>
            <kind>1</kind>
            <string>/// Input Prediction

if (input_count &gt; 0 &amp;&amp; is_input_predicted)
{
    for (var i = 0; i &lt; entities; i++)
    {
        var entity = ds_list_find_value(entity_list, i);
        
        if (entity.object_index == obj_client_player
        &amp;&amp; entity.number = player_number)
        {
            // Update player to most recent prediction
            if (ds_queue_size(predict_history) &gt; 0)
            {
                var latest_prediction = ds_queue_tail(predict_history);
                entity.x = ds_list_find_value(latest_prediction, 1);
                entity.y = ds_list_find_value(latest_prediction, 2);
            }
            // Update Player
            if (keyboard_check(vk_left) || keyboard_check(ord('A')))
                with (entity) event_perform(ev_other, ev_user0); // Left Command
            if (keyboard_check(vk_right) || keyboard_check(ord('D')))
                with (entity) event_perform(ev_other, ev_user1); // Right Command
            if (keyboard_check(vk_up) || keyboard_check(ord('W')))
                with (entity) event_perform(ev_other, ev_user2); // Up Command
            if (keyboard_check(vk_down) || keyboard_check(ord('S')))
                with (entity) event_perform(ev_other, ev_user3); // Down Command
            
            // Queue Prediction
            var prediction = ds_list_create();
            ds_list_add(prediction, cmd_id);
            ds_list_add(prediction, entity.x);
            ds_list_add(prediction, entity.y);
            ds_queue_enqueue(predict_history, prediction);
        }
    }
}

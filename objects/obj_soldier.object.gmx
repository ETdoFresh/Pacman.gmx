            <string>/// Move towards target

if (target != 0)
{
    if(target.x &gt; x) facing = RIGHT;
    if(target.x &lt; x) facing = LEFT;
    if(target.y &gt; y) facing = DOWN;
    if(target.y &lt; y) facing = UP;


        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Receive Input Command

if (type == network_type_data)
{
    var socket = eventid;
    var buffer = ds_map_find_value(async_load, "buffer");
    command = buffer_read(buffer, buffer_u8);
    
    if (command == CMD_INPUT)
    {
        var player = -1;
        for (var i = 0; i &lt; ds_list_size(entity_list); i++)
        {
            player = ds_list_find_value(entity_list, i);
            if (player.object_index == obj_player)
                if (player.socket == socket)
                    break;
            player = -1;
        }
        if (player == -1) exit;
        
        player.input_id = buffer_read(buffer, buffer_u16);
        player.input_steps = 0;
        var input_count = buffer_read(buffer, buffer_u8);
        
        for (var i = 0; i &lt; input_count; i++)
        {
            var input = buffer_read(buffer, buffer_s16);
            var is_pressed = buffer_read(buffer, buffer_bool);
            
            if (input == vk_left)
                player.key_left = is_pressed;
            else if (input == vk_right)
                player.key_right = is_pressed;
            else if (input == vk_up)
                player.key_up = is_pressed;
            else if (input == vk_down)
                player.key_down = is_pressed;
            
            // Mouse Button Pressed
            if (input == mb_left)
            {
                // Add bullet entity
                var bullet = instance_create(player.x + 8, player.y + 16, obj_bullet);
                bullet.target_x = buffer_read(buffer, buffer_s16);
                bullet.target_y = buffer_read(buffer, buffer_s16);
                bullet.image_angle = point_direction(player.x + 8, player.y + 16, bullet.target_x, bullet.target_y);
                bullet.image_alpha = 0.25;
                bullet.alarm[0] = room_speed;
                bullet.number = player.number;
                ds_list_add(entity_list, bullet);
            }
        }
    }
}

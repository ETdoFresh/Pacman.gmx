    buffer_write(player_buffer, buffer_u8, CMD_CHAT);
    buffer_write(player_buffer, buffer_u8, player.number);
    buffer_write(player_buffer, buffer_string, chat_text);
    var buffer_size = buffer_tell(player_buffer);
    
    for (var i = 0; i &lt; ds_list_size(player_list); i++)
    {
        var player = ds_list_find_value(player_list, i);
        network_send_packet(player.socket, player_buffer, buffer_size);
    }
}
